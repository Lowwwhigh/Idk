local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Get faster every click script", "BloodTheme")



---Tabs

local Tab = Window:NewTab("Auto things")
local Section = Tab:NewSection("Auto things")



----Buttons

Section:NewButton("Auto click", "Auto clicke for you", function()

while true do
     wait()game:GetService("ReplicatedStorage").Remotes.Click:FireServer()
 end

end)


Section:NewButton("Auto reb", "Idk", function()

while true do
     wait() game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
 end

end)
